<%= form_for(@inquiry) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :subject %>
  <%= f.text_field :subject %><br>

  <%= f.label :description %>
  <%= f.text_field :description %><br>

  <%= f.submit 'Submit Inquiry' %>
<% end %>
